[settings]
arch=armv7
build_type=Release
compiler=gcc
compiler.libcxx=libstdc++
compiler.version=11
os=Linux
[options]
[buildenv]
ARCH=arm
CROSS_COMPILE=arm-none-linux-gnueabihf-
SYSROOT=/opt/arm-none-linux-gnueabihf
TARGET_PREFIX=arm-none-linux-gnueabihf-

CC=arm-none-linux-gnueabihf-gcc
CXX=arm-none-linux-gnueabihf-g++
CPP=arm-none-linux-gnueabihf-gcc -E
AS=arm-none-linux-gnueabihf-as
LD=arm-none-linux-gnueabihf-ld
GDB=arm-none-linux-gnueabihf-gdb
STRIP=arm-none-linux-gnueabihf-strip
RANLIB=arm-none-linux-gnueabihf-ranlib
OBJCOPY=arm-none-linux-gnueabihf-OBJCOPY
OBJDUMP=arm-none-linux-gnueabihf-OBJDUMP
READELF=arm-none-linux-gnueabihf-READELF
AR=arm-none-linux-gnueabihf-ar
NM=arm-none-linux-gnueabihf-nm
M4=arm-none-linux-gnueabihf-m4
