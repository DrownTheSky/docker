[settings]
os=Linux
os_build=Linux
arch=armv7
arch_build=x86_64
compiler=gcc
compiler.version=11
compiler.libcxx=libstdc++
build_type=Release
[options]
*:shared=True
[build_requires]
[env]
ARCH=arm
CROSS_COMPILE=arm-none-linux-gnueabihf-
SYSROOT=/opt/arm-none-linux-gnueabihf
TARGET_PREFIX=arm-none-linux-gnueabihf-

CC="/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-gcc -fstack-protector-strong  -O2 -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security"
CXX="/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-g++ -fstack-protector-strong  -O2 -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security"
CPP="/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-gcc -E -fstack-protector-strong  -O2 -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security"
AS=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-as
LD=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-ld
GDB=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-gdb
STRIP=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-strip
RANLIB=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-ranlib
OBJCOPY=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-OBJCOPY
OBJDUMP=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-OBJDUMP
READELF=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-READELF
AR=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-ar
NM=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-nm
M4=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-m4
OBJDUMP=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-OBJDUMP
OBJDUMP=/opt/arm-none-linux-gnueabihf/bin/arm-none-linux-gnueabihf-OBJDUMP

CFLAGS=" -O2 -pipe -g -feliminate-unused-debug-types "
CXXFLAGS=" -O2 -pipe -g -feliminate-unused-debug-types "
LDFLAGS=" -Wl,-O1 -Wl,--hash-style=gnu -Wl,--as-needed  -Wl,-z,relro,-z,now "
CPPFLAGS=""
