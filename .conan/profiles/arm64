[settings]
os=Linux
os_build=Linux
arch=armv8
arch_build=x86_64
compiler=gcc
compiler.version=10
compiler.libcxx=libstdc++
build_type=Release
[options]
*:shared=True
[build_requires]
[env]
ARCH=arm64
CROSS_COMPILE=aarch64-none-linux-gnu-
SYSROOT=/opt/aarch64-none-linux-gnu
TARGET_PREFIX=aarch64-none-linux-gnu-

CC="/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-gcc -march=armv8.2-a -fstack-protector-strong  -O2 -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security"
CXX="/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-g++ -march=armv8.2-a -fstack-protector-strong  -O2 -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security"
CPP="/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-gcc -E -march=armv8.2-a -fstack-protector-strong  -O2 -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security"
AS=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-as
LD=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-ld
GDB=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-gdb
STRIP=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-strip
RANLIB=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-ranlib
OBJCOPY=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-OBJCOPY
OBJDUMP=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-OBJDUMP
READELF=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-READELF
AR=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-ar
NM=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-nm
M4=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-m4
OBJDUMP=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-OBJDUMP
OBJDUMP=/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-OBJDUMP

CFLAGS=" -O2 -pipe -g -feliminate-unused-debug-types "
CXXFLAGS=" -O2 -pipe -g -feliminate-unused-debug-types "
LDFLAGS=" -Wl,-O1 -Wl,--hash-style=gnu -Wl,--as-needed  -Wl,-z,relro,-z,now "
CPPFLAGS=""
